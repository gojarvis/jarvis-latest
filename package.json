{
  "name": "jarvis-latest",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "start": "./start.sh",
    "stop": "./stop.sh",
    "restart": "pm2 restart all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm build:apps && npm build:join",
    "build:apps": "npm run build:apps:client && npm run build:apps:server",
    "build:apps:client": "cd client && npm run build",
    "build:apps:server": "cd server && npm run build",
    "build:clean": "rm -rf app/src",
    "build:join": "cp resources/* ./app/src/ && cp server/build/* ./app/src/ && cp -r server/config ./app/src/config && cp -r client/build/* ./app/src/client",
    "build:mac": "nwbuild -p \"osx64\" -o ./build-nwjs ./server",
    "build:windows": "nwbuild -p \"win64\" -o \".\\build-nwjs\" .\\app\\src",
    "build:move": "cp -r ./app/src/* E:/code/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gojarvis/jarvis-latest.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gojarvis/jarvis-latest/issues"
  },
  "homepage": "https://github.com/gojarvis/jarvis-latest#readme",
  "dependencies": {
    "express": "^4.14.0",
    "nodemon": "^1.10.0",
    "passport-github": "^1.1.0",
    "pm2": "^1.1.3"
  },
  "devDependencies": {
    "nw-builder": "^3.0.0"
  }
}
