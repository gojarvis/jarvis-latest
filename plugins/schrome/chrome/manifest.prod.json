{
  "version": "0.0.0",
  "name": "JarvisWatcher",
  "manifest_version": 2,
  "description": "Jarvis Chrome Extension",
  "browser_action": {
    "default_title": "React Chrome Extension Example",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [ "contextMenus", "tabs", "storage", "*://localhost/*"]
}
